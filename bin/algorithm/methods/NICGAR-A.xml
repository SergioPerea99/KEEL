<algorithm_specification>
  <name>NICGAR</name>
  <nParameters>7</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
   <parameter>
    <name>Number of Evaluations</name>
    <type>integer</type>
    <domain>
      <lowerB>10000</lowerB>
      <upperB>1000000</upperB>
    </domain>
    <default>100000</default>
  </parameter>
  <parameter>
    <name>Population Size</name>
    <type>integer</type>
    <domain>
      <lowerB>2</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>100</default>
  </parameter>
  <parameter>
    <name>Probability of Mutation</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.1</default>
  </parameter>    
  <parameter>
    <name>The factor of amplitude for each attribute of the dataset</name>
    <type>real</type>
    <domain>
      <lowerB>1.0</lowerB>
      <upperB>20.0</upperB>
    </domain>
    <default>3.0</default>
  </parameter>
  <parameter>
    <name>Niching Threshold</name>
    <type>real</type>
    <domain>
      <lowerB>0.0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.5</default>
  </parameter>
  <parameter>
    <name>Quality Threshold</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1.0</upperB>
    </domain>
    <default>0.85</default>
  </parameter>
  <parameter>
    <name>Percent Update</name>
    <type>real</type>
    <domain>
      <lowerB>5.0</lowerB>
      <upperB>100.0</upperB>
    </domain>
    <default>5.0</default>
  </parameter>
</algorithm_specification>

